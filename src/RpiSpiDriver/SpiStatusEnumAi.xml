<?xml version="1.0" encoding="UTF-8"?>
<enum namespace = "Drv" name="SpiStatus">
    <comment>Enum of the status returns from the SPI Driver</comment>
    <item name="SPI_OK" value="0" comment="Transaction okay"/>
    <item name="SPI_BUSY_ERR" value="1" comment="A SPI transaction is already running"/>
    <item name="SPI_CHIP_SELECT_ERR" value="2" comment="Invalid chip select"/>
    <item name="SPI_SIZE_MISMATCH_ERR" value="3" comment="Read/Write buffers have differing sizes"/>
    <item name="SPI_TRANSACT_POLLING_ERR" value="4" comment="SPI polling transaction failed"/>
    <item name="SPI_TRANSACT_DMA_ERR" value="5" comment="SPI DMA transaction failed"/>
    <item name="SPI_OTHER_ERR" value="6" comment="Other errors that don't fit"/>
</enum>
