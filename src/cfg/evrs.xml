<?xml version="1.0" ?>
<evrs>
    <evr name="Fatal_0" format="Fatal %s:%d %s">
        <arg type="string">file</arg>
        <arg type="u32">line</arg>
        <arg type="string">expr_str</arg>
    </evr>
    <evr name="Fatal_1" format="Fatal %s:%d %s %d">
        <arg type="string">file</arg>
        <arg type="u32">line</arg>
        <arg type="string">expr_str</arg>
        <arg type="u32">arg0</arg>
    </evr>
    <evr name="Fatal_2" format="Fatal %s:%d %s %d %d">
        <arg type="string">file</arg>
        <arg type="u32">line</arg>
        <arg type="string">expr_str</arg>
        <arg type="u32">arg0</arg>
        <arg type="u32">arg1</arg>
    </evr>
    <evr name="Fatal_3" format="Fatal %s:%d %s %d %d">
        <arg type="string">file</arg>
        <arg type="u32">line</arg>
        <arg type="string">expr_str</arg>
        <arg type="u32">arg0</arg>
        <arg type="u32">arg1</arg>
        <arg type="u32">arg2</arg>
    </evr>

    <evr name="CarBoot" format="VO Car is booting up. Build %s ">
        <arg type="string" comment="Compile date, use __DATE__">compile_date</arg>
        <arg type="string" comment="Compile time, use __TIME__">compile_time</arg>
    </evr>

    <evr name="RootMounted" format="Mounted %s partition to root filesystem">
        <arg type="string" comment="Name of root partition on SDCard">partition</arg>
    </evr>

    <evr name="DpOpened" format="Opened data product %s">
        <arg type="string" comment="Path to open data product file">path</arg>
    </evr>
    <evr name="DpOpenedFailed" format="Failed to open data product %s">
        <arg type="string" comment="Data product path that failed to open">path</arg>
    </evr>
    <evr name="DpClosed" format="Closed data product %s">
        <arg type="string" comment="Path to closed data product file">path</arg>
    </evr>
    <evr name="DpWriteFailed" format="Failed to write %d bytes to %s: %d">
        <arg type="u32" comment="number of bytes attempted to write">n_bytes</arg>
        <arg type="string" comment="Path to data product file">path</arg>
        <arg type="s32" comment="Error number from file system">error</arg>
    </evr>
    <evr name="DpSyncFailed" format="Failed to synchronize data product to FATFS %s: %d">
        <arg type="string" comment="Path to data product file">path</arg>
        <arg type="s32" comment="Error number from file system">error</arg>
    </evr>
</evrs>
